@model IEnumerable<BoardGames.ViewModels.BoardGamePlayersGroup>
           
@{
    ViewBag.Title = "Statystyki gier";
}

<h2>Statystyki gier</h2>

<table>
    <tr>
        <th>
            Gra
        </th>
        <th>
            Ilość graczy
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BoardGame.Name)
        </td>
        <td>
            @item.Players
        </td>
    </tr>
}
</table>